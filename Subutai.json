{
  "name": "${envname}",
  "description": "Test blueprint with minimum parameters",
  "version": "1.0.1",
  "author": "Zubaidullo",
  "containers": [
    {
      "hostname": "apache",
      "template": "apache",
      "peer-criteria": "apache",
      "size": "MEDIUM"
    }
  ],
  "peer-criteria": [
    {
      "name": "apache",
      "max-price": "20",
      "avg-cpu-load": "50",
      "min-free-ram": "128",
      "min-free-disk-space": "10"
    }
  ],
  "ansible-configuration": {},
  "user-variables": {
    "envname": {
      "description": "Enter your environment name",
      "type": "string",
      "default": "simpleEnvironment",
      "validation": "[a-zA-Z0-9]+"
    }}
}
